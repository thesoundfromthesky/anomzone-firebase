{
  "name": "bulletin-board",
  "version": "0.0.0",
  "scripts": {
    "tsc": "tsc",
    "ng": "ng",
    "start": "ng serve",
    "start:emu": "ng serve --configuration=production,emulator",
    "start:staging": "ng serve --configuration=production,staging",
    "build": "ng build",
    "build:emu": "ng build --configuration=production,emulator",
    "build:staging": "ng build --configuration=production,staging",
    "build:prod": "ng build --prod",
    "deploy:staging": "ng build --configuration=production,staging && firebase deploy -P staging --only=hosting:staging,firestore",
    "deploy:prod": "ng build --prod && firebase deploy -P prod --only=hosting:prod,firestore",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "emu": "firebase emulators:start",
    "emu:import": "firebase emulators:start --import seed",
    "emu:export": "firebase emulators:export seed",
    "json": "svarog 'schemas/*.json' 'firestore.rules' --verbose"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.6",
    "@angular/cdk": "^10.0.0",
    "@angular/common": "~10.0.6",
    "@angular/compiler": "~10.0.6",
    "@angular/core": "~10.0.6",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "~10.0.6",
    "@angular/platform-browser": "~10.0.6",
    "@angular/platform-browser-dynamic": "~10.0.6",
    "@angular/router": "~10.0.6",
    "@ngxs/store": "^3.6.2",
    "firebase": "^7.13.1",
    "primeflex": "^2.0.0-rc.1",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.0-rc.4",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1100",
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "~10.0.6",
    "@schematics/angular": "~10.0.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.54",
    "codelyzer": "^6.0.0",
    "firebase-tools": "^8.10.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jasmine-ts": "^0.3.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "open": "^7.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^3.9.7"
  }
}
